<ResourceDictionary	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

	<SolidColorBrush	po:Freeze="true"	x:Key="ConditionBackground"				Color="Transparent"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="ConditionForeground"				Color="Black"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="MAGridForeground"				Color="#FF000000"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="MAGridLines"						Color="#FF000000"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="LabelRowBackground"				Color="Transparent"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="LabelRowForeground"				Color="#FF000000"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="RowChangeHighlightBackground"	Color="Yellow"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="RowChangeHighlightForeground"	Color="#FF000000"/>
	<SolidColorBrush	po:Freeze="true"	x:Key="MAGridTotalRowBackground" 		Color="#FFF5F5F5" />
</ResourceDictionary>