		<LinearGradientBrush.GradientStops>
			<GradientStop Color="{StaticResource WindowBottomGradientStopColor}" Offset="0"/>
			<GradientStop Color="{StaticResource WindowBottomGradientStopColor}" Offset="1"/>
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>

	<SolidColorBrush		po:Freeze="true"	x:Key="TabControlBorderBrush"		Color="#FF000000"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="TabBackground"				Color="#FFFFFFFF"/>
