	<SolidColorBrush		po:Freeze="true"	x:Key="MenuBorderBrush"				Color="#FF333333"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="MenuRibbonBrush"				Color="#33B3B3B3"/>
