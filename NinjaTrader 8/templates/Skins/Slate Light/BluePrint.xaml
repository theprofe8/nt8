	<LinearGradientBrush	po:Freeze="true"	x:Key="ScrollBarHorizontalForePressed"	StartPoint="0.5,0"	EndPoint="0.5,1"	GradientStops="{StaticResource ScrollThumbBackStopsPressed}"/>

