	<LinearGradientBrush	po:Freeze="true"	x:Key="TreeItemSelectedBackground"	StartPoint="0.5,-0.34" EndPoint="0.5,1.4" GradientStops="{StaticResource ItemHoverStops}"/>
	<SolidColorBrush 		po:Freeze="true" 	x:Key="TreeViewDisabledSelectedForeground" 	Color="#FF999999" />

