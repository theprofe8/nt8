	<SolidColorBrush		po:Freeze="true"	x:Key="ScrollBarForeAdornerDisabled"	Color="#FFCCCCCC"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="ScrollBarCaratNormal"			Color="#FF000000"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="ScrollBarCaratHover"				Color="#FF000000"/>
