	<SolidColorBrush		po:Freeze="true"	x:Key="ListBoxBackgroundBrush"		Color="#00FFFFFF"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="ListBoxItemForeground"		Color="#FF333333"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="ListBoxItemHoverBorder"		Color="{StaticResource ColorItemHoverBorder}"/>
