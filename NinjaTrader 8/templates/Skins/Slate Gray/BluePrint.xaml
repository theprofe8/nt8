	<SolidColorBrush		po:Freeze="true"	x:Key="ComboBoxItemForeground"			Color="#FFCCCCCC"/>
	<SolidColorBrush		po:Freeze="true"	x:Key="ComboBoxItemHoverBorder"			Color="{StaticResource ColorItemHoverBorder}"/>
